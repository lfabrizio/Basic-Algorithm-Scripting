function repeatStringNumTimes(str, num) {
  // repeat after me
  // empty variable to store
  var repeat = "";
  // while loop to rpeart as many times as num input asks
  while(num > 0){
    repeat += str; // plus equals adds string to variable
    num--; //decrement
  }
  return repeat;
}

repeatStringNumTimes("abc", 3);
