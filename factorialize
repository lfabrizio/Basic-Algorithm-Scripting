// factorialize non-recursion
// refactored
// non-recursion 
function factorialize(num) {
   // create an empty array 
   var arr = [];
  var factor = 1;
// for loop to iterate through each number decrementing
for(var i = num; i > 0; i--){
  arr.push(i);  // push each number to the empty array
  factor=factor*i;  // multiply
}

  return factor;
}

factorialize(5);
