function factorialize(num) {
  // non-recursion 
  var factor = 1; // counter
  var arr = []; // empty array to store
// for loop to iterate the function input 
for(var i = num; i > 0; i--) {
  arr.push[i]; // push each number to empty array
      factor = factor * i; // lastly multiplication
}

  return factor;
}

factorialize(5);
